        {
            "path": "DocumentReference.type.descendants()",
            "method": "keep"
        },
        {
            "path": "DocumentReference.subject",
            "method": "cryptoHash"
        },
        {
            "path": "DocumentReference.subject.descendants()",
            "method": "keep"
        },
        {
            "path": "DocumentReference.content",
            "method": "keep"
        },
        {
            "path": "Consent.descendants()",
            "method": "keep"
        }
